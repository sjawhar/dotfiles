{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "claude_code": {
    "mcp": true,
    "commands": false,
    "skills": false,
    "agents": false,
    "hooks": false,
    "plugins": false
  },
  "sisyphus_agent": {
    "default_builder_enabled": true,
    "planner_enabled": true,
    "replace_plan": true
  },
  "agents": {
    "sisyphus": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "max"
    },
    "hephaestus": {
      "model": "openai/gpt-5.2-codex",
      "variant": "high"
    },
    "prometheus": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "max"
    },
    "metis": {
      "model": "openai/gpt-5.2-codex",
      "variant": "high"
    },
    "momus": {
      "model": "openai/gpt-5.2",
      "variant": "high"
    },
    "oracle": {
      "model": "openai/gpt-5.2",
      "variant": "high"
    },
    "atlas": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "max"
    },
    "librarian": {
      "model": "anthropic/claude-sonnet-4-5"
    },
    "explore": {
      "model": "anthropic/claude-haiku-4-5"
    },
    "multimodal-looker": {
      "model": "google/gemini-3-flash"
    }
  },
  "categories": {
    "ultrabrain": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "max"
    },
    "deep": {
      "model": "openai/gpt-5.2-codex",
      "variant": "medium"
    },
    "visual-engineering": {
      "model": "google/gemini-3-pro"
    },
    "quick": {
      "model": "anthropic/claude-haiku-4-5"
    },
    "unspecified-low": {
      "model": "anthropic/claude-sonnet-4-5"
    },
    "unspecified-high": {
      "model": "anthropic/claude-opus-4-6",
      "variant": "max"
    },
    "writing": {
      "model": "google/gemini-3-flash"
    },
    "artistry": {
      "model": "google/gemini-3-pro",
      "variant": "high"
    }
  },
  "auto_update": false,
  "disabled_hooks": [
    "directory-readme-injector",
    "think-mode",
    "preemptive-compaction",
    "rules-injector",
    "ralph-loop",
    "tasks-todowrite-disabler",
    "auto-slash-command",
    "auto-update-checker"
  ],
  "disabled_commands": [],
  "disabled_mcps": [],
  "disabled_skills": [],
  "disabled_tools": [],
  "tmux": {
    "enabled": true,
    "layout": "main-vertical"
  },
  "_migrations": [
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex",
    "model-version:openai/gpt-5.2-codex->openai/gpt-5.3-codex"
  ]
}
